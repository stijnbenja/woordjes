    "import random\n",
    "\n",
    "d = dict(a=1, b=2, c=3, d=4, e=5, f=6, g=7)\n",
    "\n",
    "def shuffled_dict(d:dict):\n",
    "    return {k: d[k] for k in random.sample(d.keys(), len(d))}\n",
